# Server settings
PORT=3001

# KuCoin API credentials (required for live trading)
KUCOIN_API_KEY=your_api_key
KUCOIN_API_SECRET=your_api_secret
KUCOIN_API_PASSPHRASE=your_api_passphrase

# Optional demo settings
# Set DEMO_MODE=true to run with synthetic data and mock API responses.
# No live orders will be sent when demo mode is enabled.
DEMO_MODE=false
# Disable recurring background intervals for testing purposes
RUN_INTERVALS=true

# Feature Flags (V4.0)
# Dual-timeframe screener for multi-symbol scanning
SCREENER_ENABLED=false
# Depth of Market (DOM) integration for order book analytics
DOM_ENABLED=false
# Strict rate limiting with adaptive token bucket
RATE_LIMIT_STRICT=true
# Use WebSocket for live market data feed
DATA_FEED_WS=true
# Live trading mode (false = paper trading/backtest)
LIVE_MODE=false
# Forward testing mode (shadow execution without real orders)
FORWARD_TEST=false
